ğŸ“„ æ–‡ä»¶ 2: core/config_manager.py
æ ¸å¿ƒé€»è¾‘ï¼šè¿™é‡Œå®ç°äº†â€œé…ç½®åˆ†å±‚â€ã€‚è¯»å–æ—¶åˆå¹¶ï¼ˆä¸ªäººä¼˜å…ˆï¼‰ï¼Œå†™å…¥æ—¶åªå†™ä¸ªäººæ–‡ä»¶ï¼Œä¿æŠ¤å…±äº«é…ç½®ä¸è¢«ä¿®æ”¹ã€‚
import json
import os
import uuid
from typing import Dict, Any, List

class ConfigManager:
    def __init__(self, team_path: str, user_path: str):
        self.team_path = team_path
        self.user_path = user_path

    def _load_json(self, path: str) -> Dict:
        if not os.path.exists(path):
            return {}
        try:
            with open(path, 'r', encoding='utf-8') as f:
                return json.load(f)
        except Exception:
            return {}

    def _save_json(self, path: str, data: Dict):
        with open(path, 'w', encoding='utf-8') as f:
            json.dump(data, f, indent=4, ensure_ascii=False)

    def get_all_templates(self) -> Dict[str, Any]:
        """åˆå¹¶é…ç½®ï¼šTeam + User"""
        team_data = self._load_json(self.team_path)
        user_data = self._load_json(self.user_path)

        merged = {}
        # 1. è½½å…¥å›¢é˜Ÿæ¨¡ç‰ˆ (æ ‡è®°ä¸ºåªè¯»)
        for k, v in team_data.items():
            v['id'] = k
            v['source'] = 'team'
            v['category'] = v.get('category', 'å…¬å…±æ¨¡ç‰ˆ')
            merged[k] = v
            
        # 2. è½½å…¥ä¸ªäººæ¨¡ç‰ˆ (åŒåä¼šè¦†ç›–å›¢é˜Ÿé…ç½®)
        for k, v in user_data.items():
            v['id'] = k
            v['source'] = 'personal'
            v['category'] = v.get('category', 'ä¸ªäººæ¨¡ç‰ˆ')
            merged[k] = v
            
        return merged

    def save_template(self, template_id: str, template_data: Dict):
        """ä¿å­˜æ¨¡ç‰ˆï¼šåªå†™å…¥ user_config.json"""
        user_data = self._load_json(self.user_path)
        
        # ç¡®ä¿ä¿å­˜è¿›å»çš„æ•°æ®å¹²å‡€
        save_payload = {
            "name": template_data.get("name"),
            "desc": template_data.get("desc"),
            "category": template_data.get("category"),
            "template": template_data.get("template"),
            "params": template_data.get("params"),
            "updated_at": "now" # ç®€å•æ ‡è®°
        }
        
        user_data[template_id] = save_payload
        self._save_json(self.user_path, user_data)

    def delete_template(self, template_id: str):
        """åˆ é™¤æ¨¡ç‰ˆï¼šåªèƒ½åˆ é™¤ä¸ªäººçš„"""
        user_data = self._load_json(self.user_path)
        if template_id in user_data:
            del user_data[template_id]
            self._save_json(self.user_path, user_data)
            return True
        return False

ğŸ“„ æ–‡ä»¶ 3: modules/cmd_generator/app.py
æ¨¡å—é€»è¾‘ï¼šå®ç°äº†ä½ è¦æ±‚çš„â€œå­å¯¼èˆªæŠ˜å åˆ†ç±»â€ã€â€œæ–°å¢åˆ†ç±»ä¸‹æ‹‰æ¡†â€ä»¥åŠâ€œä¸‰æ å¸ƒå±€çš„ä¸­é—´æ å’Œå³ä¾§å†…å®¹â€ã€‚
import streamlit as st
import pandas as pd
from jinja2 import Template
import shlex
import uuid

# å¸¸é‡å®šä¹‰
MODE_LIST = "ğŸ“‚ æ¨¡æ¿åˆ—è¡¨"
MODE_ADMIN = "ğŸ› ï¸ ç®¡ç†æ¨¡æ¿"

def render(cfg_mgr):
    """
    æ¨¡å—å…¥å£å‡½æ•°
    """
    # 1. è·å–æœ€æ–°æ•°æ®
    templates = cfg_mgr.get_all_templates()
    
    # åˆå§‹åŒ– Session State
    if 'selected_template_id' not in st.session_state:
        st.session_state['selected_template_id'] = None
    if 'nav_mode' not in st.session_state:
        st.session_state['nav_mode'] = MODE_LIST

    # ==================================================
    # [å­å¯¼èˆªæ ] (å¯¹åº”ä¸‰æ å¸ƒå±€çš„ä¸­é—´æ )
    # ==================================================
    with st.sidebar:
        st.header("ç”Ÿæˆå™¨èœå•")
        
        # åˆ‡æ¢æ¨¡å¼ï¼šæµè§ˆ vs ç®¡ç†
        mode = st.radio("æ¨¡å¼", [MODE_LIST, MODE_ADMIN], label_visibility="collapsed")
        st.session_state['nav_mode'] = mode
        st.markdown("---")

        if mode == MODE_LIST:
            st.caption("æŒ‰åˆ†ç±»æµè§ˆ")
            # æå–æ‰€æœ‰åˆ†ç±»å¹¶æ’åº
            all_cats = sorted(list(set(t['category'] for t in templates.values())))
            
            for cat in all_cats:
                # ç­›é€‰è¯¥åˆ†ç±»ä¸‹çš„æ¨¡ç‰ˆ
                cat_items = [t for t in templates.values() if t['category'] == cat]
                
                # æ ¸å¿ƒéœ€æ±‚ï¼šå¯æŠ˜å åˆ†ç±»åˆ—è¡¨
                with st.expander(f"ğŸ“ {cat} ({len(cat_items)})", expanded=False):
                    for item in cat_items:
                        # é€‰ä¸­æŒ‰é’®
                        label = item.get('name', 'æœªå‘½å')
                        if st.button(label, key=f"btn_{item['id']}", use_container_width=True):
                            st.session_state['selected_template_id'] = item['id']

        elif mode == MODE_ADMIN:
            st.caption("æ¨¡æ¿ç»´æŠ¤")
            if st.button("â• æ–°å»ºç©ºæ¨¡æ¿", use_container_width=True):
                st.session_state['selected_template_id'] = "NEW_TEMPLATE"

    # ==================================================
    # [ä¸»å†…å®¹åŒº] (å¯¹åº”ä¸‰æ å¸ƒå±€çš„å³ä¾§è¯¦æƒ…)
    # ==================================================
    current_id = st.session_state.get('selected_template_id')

    # è·¯ç”±é€»è¾‘
    if st.session_state['nav_mode'] == MODE_ADMIN:
        # è¿›å…¥ç®¡ç†ç•Œé¢
        render_admin_ui(cfg_mgr, templates, current_id)
    else:
        # è¿›å…¥ä½¿ç”¨ç•Œé¢
        if current_id and current_id in templates:
            render_generator_ui(templates[current_id])
        else:
            st.info("ğŸ‘ˆ è¯·åœ¨å·¦ä¾§ä¾§è¾¹æ é€‰æ‹©ä¸€ä¸ªæ¨¡æ¿å¼€å§‹ä½¿ç”¨")

# ------------------------------------------------------
# UI ç»„ä»¶ï¼šç”Ÿæˆå™¨ç•Œé¢
# ------------------------------------------------------
def render_generator_ui(config):
    st.subheader(f"ğŸš€ {config.get('name')}")
    st.caption(config.get('desc'))
    
    # æ¥æºæ ‡è¯†
    if config.get('source') == 'team':
        st.markdown("`ğŸ¢ å›¢é˜Ÿå…¬å…±æ¨¡ç‰ˆ`")
    else:
        st.markdown("`ğŸ‘¤ ä¸ªäººç§æœ‰æ¨¡ç‰ˆ`")

    st.markdown("---")
    
    # åŠ¨æ€è¡¨å•
    user_inputs = {}
    params = config.get("params", [])
    
    with st.form(key=f"form_{config['id']}"):
        col1, col2 = st.columns(2)
        for idx, p in enumerate(params):
            col = col1 if idx % 2 == 0 else col2
            p_name = p.get("name")
            p_label = p.get("label", p_name)
            p_type = p.get("type", "text")
            p_default = p.get("default", "")
            
            if p_type == "text":
                user_inputs[p_name] = col.text_input(p_label, value=str(p_default))
            elif p_type == "number":
                user_inputs[p_name] = col.number_input(p_label, value=float(p_default) if p_default else 0.0)
            elif p_type == "select":
                opts = [x.strip() for x in p.get("options", "").split(",") if x.strip()]
                user_inputs[p_name] = col.selectbox(p_label, options=opts)
        
        submitted = st.form_submit_button("ç”Ÿæˆå‘½ä»¤", use_container_width=True)
    
    if submitted:
        try:
            # å®‰å…¨å¤„ç†
            safe_inputs = {k: (shlex.quote(v) if isinstance(v, str) else v) for k, v in user_inputs.items()}
            t = Template(config.get("template", ""))
            res = t.render(**safe_inputs)
            st.success("ç”Ÿæˆç»“æœï¼š")
            st.code(res, language="bash")
        except Exception as e:
            st.error(f"æ¸²æŸ“å¤±è´¥: {e}")

# ------------------------------------------------------
# UI ç»„ä»¶ï¼šç®¡ç†ç•Œé¢
# ------------------------------------------------------
def render_admin_ui(cfg_mgr, templates, current_id):
    st.header("ğŸ› ï¸ æ¨¡æ¿ç¼–è¾‘å™¨")

    # æ•°æ®å‡†å¤‡
    is_new = (current_id == "NEW_TEMPLATE")
    data = {}
    
    if not is_new and current_id in templates:
        data = templates[current_id]
        if data.get('source') == 'team':
            st.warning("âš ï¸ ä½ æ­£åœ¨æŸ¥çœ‹ä¸€ä¸ªã€å›¢é˜Ÿå…¬å…±æ¨¡ç‰ˆã€‘ã€‚ä¿å­˜åå®ƒå°†ä½œä¸ºã€ä¸ªäººæ¨¡ç‰ˆã€‘å¦å­˜ä¸€ä»½ï¼Œä¸ä¼šä¿®æ”¹åŸæ–‡ä»¶ã€‚")
    
    # --- è¡¨å•å¼€å§‹ ---
    # 1. åŸºç¡€ä¿¡æ¯
    c1, c2 = st.columns([1, 2])
    
    # æ ¸å¿ƒéœ€æ±‚ï¼šåˆ†ç±»é€‰æ‹© + æ–°å¢åŠŸèƒ½
    existing_cats = sorted(list(set(t['category'] for t in templates.values())))
    if "é»˜è®¤åˆ†ç±»" not in existing_cats: existing_cats.append("é»˜è®¤åˆ†ç±»")
    
    # ä¸‹æ‹‰æ¡†é€‰é¡¹ï¼šå¢åŠ ä¸€ä¸ªç‰¹æ®Šé€‰é¡¹
    cat_options = existing_cats + ["â• æ–°å¢åˆ†ç±»..."]
    
    default_cat_idx = 0
    if not is_new and data.get('category') in existing_cats:
        default_cat_idx = existing_cats.index(data.get('category'))
        
    selected_cat_op = c1.selectbox("æ‰€å±åˆ†ç±»", cat_options, index=default_cat_idx)
    
    # å¦‚æœé€‰ä¸­äº†æ–°å¢ï¼Œæ˜¾ç¤ºè¾“å…¥æ¡†
    final_category = selected_cat_op
    if selected_cat_op == "â• æ–°å¢åˆ†ç±»...":
        final_category = c1.text_input("è¯·è¾“å…¥æ–°åˆ†ç±»åç§°", placeholder="ä¾‹å¦‚: æ•°æ®åº“å·¡æ£€")
        if not final_category: final_category = "æœªåˆ†ç±»"

    name_val = data.get('name', '')
    input_name = c2.text_input("æ¨¡æ¿åç§°", value=name_val)
    input_desc = st.text_input("åŠŸèƒ½æè¿°", value=data.get('desc', ''))

    # 2. å‚æ•°ç¼–è¾‘å™¨ (Pandas Editor)
    st.markdown("**å‚æ•°å®šä¹‰**")
    default_df = data.get('params', [{"name": "param1", "label": "ç¤ºä¾‹å‚æ•°", "type": "text", "default": "", "options": ""}])
    
    edited_df = st.data_editor(
        pd.DataFrame(default_df),
        num_rows="dynamic",
        column_config={
            "type": st.column_config.SelectboxColumn("ç±»å‹", options=["text", "number", "select"], required=True),
            "options": st.column_config.TextColumn("é€‰é¡¹ (ä»…Selectæœ‰æ•ˆ)", help="é€—å·åˆ†éš”")
        },
        use_container_width=True
    )

    # 3. æ¨¡æ¿ä»£ç 
    st.markdown("**Jinja2 æ¨¡æ¿ä»£ç **")
    input_template = st.text_area("Code", value=data.get('template', ''), height=150)

    # 4. ä¿å­˜æŒ‰é’®
    c_btn1, c_btn2 = st.columns([1, 5])
    if c_btn1.button("ğŸ’¾ ä¿å­˜æ¨¡æ¿"):
        if not input_name:
            st.error("å¿…é¡»å¡«å†™åç§°")
            return
            
        # ç”Ÿæˆ ID (å¦‚æœæ˜¯æ–°å»ºï¼Œæˆ–è€…æ˜¯å¤åˆ¶Teamçš„ï¼Œéƒ½ç”Ÿæˆæ–°ID)
        if is_new or (data.get('source') == 'team'):
            save_id = str(uuid.uuid4())[:8]
        else:
            save_id = current_id
            
        # æ•´ç†æ•°æ®
        params_list = edited_df.to_dict(orient="records")
        params_list = [p for p in params_list if p["name"]] # è¿‡æ»¤ç©ºè¡Œ
        
        save_data = {
            "name": input_name,
            "desc": input_desc,
            "category": final_category,
            "template": input_template,
            "params": params_list
        }
        
        cfg_mgr.save_template(save_id, save_data)
        st.toast(f"ä¿å­˜æˆåŠŸ! ID: {save_id}", icon="âœ…")
        # åˆ·æ–°é¡µé¢çŠ¶æ€
        st.session_state['selected_template_id'] = save_id
        st.session_state['nav_mode'] = MODE_LIST # ä¿å­˜åè·³å›åˆ—è¡¨çœ‹æ•ˆæœ
        st.rerun()

    # åˆ é™¤æŒ‰é’® (ä»…ä¸ªäººæ¨¡ç‰ˆå¯è§)
    if not is_new and data.get('source') == 'personal':
        if c_btn2.button("ğŸ—‘ï¸ åˆ é™¤", type="primary"):
            cfg_mgr.delete_template(current_id)
            st.session_state['selected_template_id'] = None
            st.rerun()

ğŸ“„ æ–‡ä»¶ 4: main.py
å…¥å£é€»è¾‘ï¼šè¿™æ˜¯æœ€å¤–å±‚çš„æ¡†æ¶ï¼Œè´Ÿè´£ç¬¬ä¸€çº§å¯¼èˆªï¼ˆç²—çš„é‚£ä¸€æ¡ï¼‰ï¼Œå¹¶å°†ä¸Šä¸‹æ–‡ä¼ é€’ç»™æ¨¡å—ã€‚
import streamlit as st
import os
from core.config_manager import ConfigManager
# å¯¼å…¥ä¸šåŠ¡æ¨¡å—
from modules.cmd_generator import app as cmd_app

# --- å…¨å±€é…ç½® ---
# å»ºè®®ï¼šè¿™é‡Œå°†æ¥å¯ä»¥æ”¹æˆä»ç¯å¢ƒå˜é‡è¯»å–ï¼Œæˆ–è€…ç›´æ¥å†™æ­»ä½ çš„å…±äº«ç›˜è·¯å¾„
# ä¾‹å¦‚: TEAM_CONFIG_PATH = r"Z:\Public\OpsTools\team_config.json"
TEAM_CONFIG_PATH = "team_config.json"
USER_CONFIG_PATH = "user_config.json"

def main():
    st.set_page_config(page_title="Ops Workstation", layout="wide", page_icon="ğŸ› ï¸")

    # åˆå§‹åŒ–é…ç½®ç®¡ç†å™¨
    cfg_mgr = ConfigManager(TEAM_CONFIG_PATH, USER_CONFIG_PATH)

    # ==========================================
    # [ä¸»å¯¼èˆªæ ] (æœ€å·¦ä¾§ï¼Œä¸€çº§å¯¼èˆª)
    # ==========================================
    with st.sidebar:
        st.title("Ops Platform")
        
        # æ¨¡å—æ³¨å†Œè¡¨
        MODULES = {
            "cmd_gen": "ğŸ“ å‘½ä»¤ç”Ÿæˆå™¨",
            "db_tools": "ğŸ—„ï¸ æ•°æ®åº“å·¥å…· (è§„åˆ’)",
            "monitor": "ğŸ“Š ç›‘æ§å¤§ç›˜ (è§„åˆ’)"
        }
        
        # ä½¿ç”¨ radio æ¨¡æ‹Ÿä¸€çº§èœå•
        selected_mod = st.radio(
            "ä¸»èœå•", 
            options=list(MODULES.keys()), 
            format_func=lambda x: MODULES[x],
            label_visibility="collapsed"
        )
        
        st.markdown("---")
        # è¿™é‡Œä»¥åå¯ä»¥æ”¾ç”¨æˆ·ä¿¡æ¯ç­‰
        st.caption(f"Config: {os.path.basename(USER_CONFIG_PATH)}")

    # ==========================================
    # [æ¨¡å—åˆ†å‘]
    # ==========================================
    if selected_mod == "cmd_gen":
        # è¿›å…¥å‘½ä»¤ç”Ÿæˆå™¨æ¨¡å—
        # æŠŠé…ç½®ç®¡ç†å™¨ä¼ è¿›å»ï¼Œè®©æ¨¡å—è‡ªå·±å»è¯»å†™æ•°æ®
        cmd_app.render(cfg_mgr)
        
    elif selected_mod == "db_tools":
        st.title("æ•°æ®åº“å·¥å…·ç®±")
        st.info("ğŸš§ æ­£åœ¨æ–½å·¥ä¸­... è¿™é‡Œå°†å±•ç¤ºè§£è€¦åˆçš„ä¼˜åŠ¿ï¼Œä»£ç å®Œå…¨ç‹¬ç«‹ã€‚")
        
    elif selected_mod == "monitor":
        st.title("ç›‘æ§ä¸­å¿ƒ")
        st.info("ğŸš§ æ­£åœ¨æ–½å·¥ä¸­...")

if __name__ == "__main__":
    main()

