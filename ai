import streamlit as st
import streamlit_antd_components as sac
import pandas as pd
from jinja2 import Template
import shlex
import uuid

def render(cfg_mgr, tag_mgr):
    # ==========================
    # 1. ç´§å‡‘å‹ Header
    # ==========================
    c_head, c_tab = st.columns([1, 2], gap="small")
    with c_head:
        st.subheader("ğŸš€ å‘½ä»¤ç”Ÿæˆå™¨")
    with c_tab:
        try:
            # å°è¯•ä½¿ç”¨ Segmented
            tab_label = sac.segmented(
                items=[
                    sac.SegmentedItem(label='Team Hub', icon='buildings'),
                    sac.SegmentedItem(label='My Workspace', icon='person-lock'),
                ], align='end', size='sm', color='indigo'
            )
        except:
            tab_label = st.radio("Source", ['Team Hub', 'My Workspace'], horizontal=True)

    source_map = {'Team Hub': 'team', 'My Workspace': 'personal'}
    current_source = source_map.get(tab_label, 'team')

    # ==========================
    # 2. ç´§å‡‘å‹ Toolbar (å•è¡Œ)
    # ==========================
    with st.container(border=True):
        # å¸ƒå±€ï¼šæœç´¢(2) | æ ‡ç­¾ç»´åº¦(1.5) | æ ‡ç­¾å€¼(1.5) | è§†å›¾(1) | æ–°å»º(1)
        c1, c2, c3, c4, c5 = st.columns([2.5, 1.5, 1.5, 1.5, 1])
        
        all_templates = cfg_mgr.get_all_templates() or {}
        all_tags = tag_mgr.get_all_tags() or {}
        
        # A. æœç´¢
        search_txt = c1.text_input("Search", placeholder="Keywords...", label_visibility="collapsed")
        
        # B. çº§è”æ ‡ç­¾ç­›é€‰
        # Step 1: æå–æ‰€æœ‰åˆ†ç±»
        all_cats = sorted(list(set(t.get('category', 'General') for t in all_tags.values())))
        sel_cat = c2.selectbox("Filter Category", ["All"] + all_cats, label_visibility="collapsed")
        
        # Step 2: æ ¹æ®åˆ†ç±»è¿‡æ»¤å¯é€‰æ ‡ç­¾
        available_tags = {k: v for k, v in all_tags.items() if (sel_cat == "All" or v.get('category') == sel_cat)}
        tag_options = {tid: f"{t.get('category','Gen')}: {t['name']}" for tid, t in available_tags.items()}
        
        sel_tags = c3.multiselect("Filter Tags", options=tag_options.keys(), format_func=lambda x: tag_options[x], placeholder="Select Tags...", label_visibility="collapsed")
        
        # C. è§†å›¾
        view_mode = c4.radio("View", ["Grid", "List"], horizontal=True, label_visibility="collapsed")
        
        # D. æ–°å»º
        if c5.button("â• New", type="primary", use_container_width=True):
            open_editor_dialog(cfg_mgr, tag_mgr, "NEW", current_source)

    # ==========================
    # 3. æ•°æ®è¿‡æ»¤
    # ==========================
    current_list = {k: v for k, v in all_templates.items() if v.get('source') == current_source}
    
    display_items = []
    for tid, t in current_list.items():
        if search_txt and (search_txt.lower() not in t['name'].lower()): continue
        
        t_tags = t.get('tags', [])
        # æ ‡ç­¾äº¤é›†ç­›é€‰
        if sel_tags:
            if not set(sel_tags).intersection(set(t_tags)): continue
        
        # æ„å»ºç”¨äºæ˜¾ç¤ºçš„æ ‡ç­¾å¯¹è±¡ (Grafana é£æ ¼)
        # æ ¼å¼: Category: Name
        display_tag_objs = []
        for tg in t_tags:
            if tg in all_tags:
                tag_meta = all_tags[tg]
                display_tag_objs.append({
                    "label": f"{tag_meta.get('category','Gen')}: {tag_meta['name']}",
                    "color": tag_meta.get('color', '#888')
                })
        
        display_items.append({
            "id": tid, "name": t['name'], "desc": t.get('desc', ''), 
            "tag_objs": display_tag_objs, "raw_data": t
        })

    # ==========================
    # 4. æ¸²æŸ“
    # ==========================
    if not display_items:
        st.info("No matching templates found.")
        return

    st.caption(f"Found {len(display_items)} templates")

    if view_mode == "List":
        render_list_view(display_items, cfg_mgr)
    else:
        render_grid_view(display_items, cfg_mgr)

# --- è§†å›¾ç»„ä»¶ ---
def render_grid_view(items, cfg_mgr):
    cols = st.columns(3)
    for idx, item in enumerate(items):
        with cols[idx % 3]:
            with st.container(border=True):
                # æ ‡é¢˜è¡Œ
                h1, h2 = st.columns([5, 1])
                h1.markdown(f"**{item['name']}**")
                # è¿è¡ŒæŒ‰é’®å°ä¸€ç‚¹
                if h2.button("â–¶", key=f"r_{item['id']}", help="Run"):
                    open_runner_dialog(cfg_mgr, item['id'])

                # æè¿°
                st.caption(item['desc'][:50] or "No description")
                
                # æ ‡ç­¾è¡Œ (Grafana é£æ ¼)
                if item['tag_objs']:
                    try:
                        sac.tags([
                            sac.Tag(label=t['label'], color=t['color'], bordered=False) 
                            for t in item['tag_objs'][:3]
                        ], size='xs', align='start')
                    except:
                        pass
                else:
                    st.caption("No Tags")
                
                # åº•éƒ¨åŠ¨ä½œ
                if st.button("Edit", key=f"e_{item['id']}", use_container_width=True):
                     open_editor_dialog(cfg_mgr, None, item['id'], item['raw_data']['source'])

def render_list_view(items, cfg_mgr):
    df_data = []
    for item in items:
        # åˆ—è¡¨æ¨¡å¼ä¸‹ï¼Œæ ‡ç­¾ç›´æ¥æ˜¾ç¤ºå­—ç¬¦ä¸²
        t_str = [t['label'] for t in item['tag_objs']]
        df_data.append({"ID": item['id'], "Name": item['name'], "Tags": t_str, "Desc": item['desc']})
    
    df = pd.DataFrame(df_data)
    
    # æ™ºèƒ½é™çº§
    try:
        tag_col = st.column_config.ListColumn("Tags")
    except:
        tag_col = st.column_config.TextColumn("Tags")

    event = st.dataframe(
        df,
        column_config={
            "ID": None,
            "Name": st.column_config.TextColumn("Name", width="medium"),
            "Tags": tag_col,
            "Desc": st.column_config.TextColumn("Description", width="large")
        },
        use_container_width=True, hide_index=True, on_select="rerun", selection_mode="single-row"
    )
    
    if len(event.selection.rows) > 0:
        open_runner_dialog(cfg_mgr, df.iloc[event.selection.rows[0]]['ID'])

# --- å¼¹çª—ç»„ä»¶ (ä¿æŒåŸæœ‰é€»è¾‘ï¼Œä»…å¾®è°ƒ UI) ---
@st.dialog("Terminal", width="large")
def open_runner_dialog(cfg_mgr, tid):
    # ... (æ­¤å¤„é€»è¾‘ä¸ä¹‹å‰ç›¸åŒï¼Œä¸ºèŠ‚çœç¯‡å¹…ç•¥å»ï¼Œè¯·ç›´æ¥å¤ç”¨ä¸Šä¸€ç‰ˆçš„ open_runner_dialog)
    # åªéœ€è¦æŠŠ title æ”¹æˆç®€å•çš„ "Terminal" å³å¯
    templates = cfg_mgr.get_all_templates()
    data = templates.get(tid)
    if not data: return st.error("Error")
    
    c1, c2 = st.columns([1, 1.2])
    user_inputs = {}
    with c1:
        st.caption("Parameters")
        with st.form("run"):
            for p in data.get('params', []):
                label = p.get('label', p['name'])
                if p['type'] == 'select':
                    opts = [x.strip() for x in p.get("options", "").split(",") if x.strip()]
                    user_inputs[p['name']] = st.selectbox(label, opts)
                else:
                    user_inputs[p['name']] = st.text_input(label, value=str(p.get('default', '')))
            sub = st.form_submit_button("Generate", type="primary", use_container_width=True)
    with c2:
        st.caption("Output")
        res = "..."
        if sub:
            try:
                safe = {k: (shlex.quote(str(v)) if isinstance(v, str) else v) for k, v in user_inputs.items()}
                t = Template(data.get('template', ''))
                res = t.render(**safe)
            except Exception as e: res = str(e)
        st.code(res, language="bash")

@st.dialog("Editor", width="large")
def open_editor_dialog(cfg_mgr, tag_mgr, tid, target_source):
    if tag_mgr is None: from core.tag_manager import TagManager; tag_mgr = TagManager("tags.json")
    
    is_new = (tid == "NEW")
    templates = cfg_mgr.get_all_templates()
    data = {} if is_new else templates.get(tid, {})
    
    name = st.text_input("Name", value=data.get('name', ''))
    
    # é«˜çº§æ ‡ç­¾é€‰æ‹©å™¨ï¼šåˆ†ç»„æ˜¾ç¤º
    all_tags = tag_mgr.get_all_tags()
    # æ ¼å¼åŒ–æ˜¾ç¤ºä¸º "Category: Name"
    tag_map = {k: f"{v.get('category','Gen')}: {v['name']}" for k, v in all_tags.items()}
    
    default_tags = [t for t in data.get('tags', []) if t in tag_map]
    sel_tags = st.multiselect("Tags", options=tag_map.keys(), default=default_tags, format_func=lambda x: tag_map[x])
    
    desc = st.text_input("Description", value=data.get('desc', ''))
    
    st.caption("Parameters (Table)")
    default_params = data.get('params', [{"name": "param", "label": "Label", "type": "text", "default": "", "options": ""}])
    edited_df = st.data_editor(pd.DataFrame(default_params), num_rows="dynamic", use_container_width=True)
    
    code = st.text_area("Template (Jinja2)", value=data.get('template', ''), height=150)
    
    if st.button("Save", type="primary", use_container_width=True):
        if not name: return st.error("Name required")
        save_id = str(uuid.uuid4())[:8] if is_new else tid
        p_list = [p for p in edited_df.to_dict('records') if p['name']]
        save_data = {"name": name, "desc": desc, "tags": sel_tags, "template": code, "params": p_list}
        cfg_mgr.save_template(save_id, save_data, target_source)
        st.rerun()

