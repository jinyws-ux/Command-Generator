ğŸ“‚ 1. core/tag_manager.py (æ•°æ®ç»“æ„å‡çº§)
æˆ‘ä»¬éœ€è¦æ”¯æŒâ€œç»´åº¦â€çš„æ¦‚å¿µã€‚
import json
import os
import uuid

class TagManager:
    def __init__(self, tag_file="tags.json"):
        self.file_path = os.path.abspath(tag_file)
        if not os.path.exists(self.file_path):
            with open(self.file_path, 'w', encoding='utf-8') as f:
                json.dump({}, f)

    def get_all_tags(self):
        try:
            with open(self.file_path, 'r', encoding='utf-8') as f:
                data = json.load(f)
                return data if isinstance(data, dict) else {}
        except:
            return {}

    def save_all_tags(self, tags_dict):
        """ä¿å­˜æ‰€æœ‰æ ‡ç­¾"""
        with open(self.file_path, 'w', encoding='utf-8') as f:
            json.dump(tags_dict, f, indent=4, ensure_ascii=False)
            
    def get_categories(self):
        """æå–æ‰€æœ‰å­˜åœ¨çš„åˆ†ç±»ç»´åº¦"""
        tags = self.get_all_tags()
        # é»˜è®¤è‡³å°‘æœ‰ä¸€ä¸ª 'General'
        cats = set(t.get('category', 'General') for t in tags.values())
        return sorted(list(cats))

ğŸ“‚ 2. main.py (åŒ…å«å…¨æ–°çš„æ ‡ç­¾ç®¡ç†å™¨ UI)
è¿™é‡Œæˆ‘é‡å†™äº† render_tag_manager_proï¼Œå®ç°äº†ä½ æƒ³è¦çš„**â€œå·¦ä¾§åˆ†ç±»æ ‘ï¼Œå³ä¾§è¯¦æƒ…è¡¨â€çš„å¸ƒå±€ï¼Œå¹¶åŠ å…¥äº†é¢œè‰²é¢„è®¾**ã€‚
import streamlit as st
import streamlit_antd_components as sac
from core.config_manager import ConfigManager
from core.tag_manager import TagManager
from modules.cmd_generator import app as cmd_app
import pandas as pd
import uuid

# è·¯å¾„é…ç½®
TEAM_CONFIG = "team_config.json"
USER_CONFIG = "user_config.json"
TAG_CONFIG = "tags.json"

# é¢œè‰²é¢„è®¾ (Grafana é£æ ¼)
COLOR_PRESETS = {
    "Critical (çº¢)": "#ff4d4f",
    "Warning (æ©™)": "#faad14",
    "Success (ç»¿)": "#52c41a",
    "Info (è“)": "#1890ff",
    "Discovery (ç´«)": "#722ed1",
    "Neutral (ç°)": "#8c8c8c",
    "Teal (é’)": "#13c2c2"
}

def inject_global_css():
    st.markdown("""
        <style>
        .block-container { padding-top: 1rem; padding-bottom: 2rem; }
        .stApp { font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif; }
        
        /* ç´§å‡‘åŒ– DataEditor */
        [data-testid="stDataFrame"] { border: 1px solid #e5e5e5; border-radius: 4px; }
        
        /* ä¾§è¾¹æ å¾®è°ƒ */
        [data-testid="stSidebar"] { border-right: 1px solid #f0f0f0; }
        </style>
    """, unsafe_allow_html=True)

def main():
    st.set_page_config(page_title="Ops Console", layout="wide", page_icon="âš¡")
    inject_global_css()

    cfg_mgr = ConfigManager(TEAM_CONFIG, USER_CONFIG)
    tag_mgr = TagManager(TAG_CONFIG)

    # === å·¦ä¾§å¯¼èˆª (SAC Menu) ===
    with st.sidebar:
        st.caption("PLATFORM")
        selected = sac.menu([
            sac.MenuItem('æ§åˆ¶å°', icon='columns-gap', children=[
                sac.MenuItem('å‘½ä»¤ç”Ÿæˆå™¨', icon='terminal'),
                sac.MenuItem('ç›‘æ§å¤§ç›˜', icon='activity', disabled=True),
            ]),
            sac.MenuItem(type='divider'),
            sac.MenuItem('é…ç½®', icon='gear', children=[
                sac.MenuItem('æ ‡ç­¾ä½“ç³»', icon='tags'),
            ]),
        ], color='indigo', open_all=True)

    # === è·¯ç”±åˆ†å‘ ===
    if selected == 'å‘½ä»¤ç”Ÿæˆå™¨':
        try:
            cmd_app.render(cfg_mgr, tag_mgr)
        except Exception as e:
            st.error(f"Render Error: {e}")
            
    elif selected == 'æ ‡ç­¾ä½“ç³»':
        render_tag_manager_pro(tag_mgr)
        
    else:
        st.title("Ops Console")
        st.info("Select a module to start.")

# ==========================================
# ğŸ·ï¸ æ ‡ç­¾ç®¡ç† Pro (Grafana é£æ ¼)
# ==========================================
def render_tag_manager_pro(tag_mgr):
    st.title("ğŸ·ï¸ æ ‡ç­¾åˆ†ç±»ä½“ç³»")
    
    # 1. å¸ƒå±€ï¼šå·¦ä¾§ç±»åˆ«å¯¼èˆªï¼Œå³ä¾§ç¼–è¾‘åŒº
    c_nav, c_editor = st.columns([1, 3], gap="large")
    
    all_tags = tag_mgr.get_all_tags()
    all_cats = sorted(list(set(t.get('category', 'General') for t in all_tags.values())))
    if "General" not in all_cats: all_cats.insert(0, "General")

    # === å·¦ä¾§ï¼šç±»åˆ«é€‰æ‹©å™¨ ===
    with c_nav:
        st.subheader("ç»´åº¦ (Dimension)")
        # ä½¿ç”¨ Radio æ¨¡æ‹Ÿä¾§è¾¹å¯¼èˆªåˆ—è¡¨
        # æ–°å¢ç±»åˆ«é€»è¾‘
        new_cat = st.text_input("â• æ–°å¢ç»´åº¦", placeholder="è¾“å…¥åå›è½¦...")
        if new_cat and new_cat not in all_cats:
            all_cats.append(new_cat)
            
        selected_cat = st.radio("é€‰æ‹©ç»´åº¦è¿›è¡Œç®¡ç†", all_cats, label_visibility="collapsed")
        st.info(f"å½“å‰ç®¡ç†: **{selected_cat}**")

    # === å³ä¾§ï¼šè¡¨æ ¼ç¼–è¾‘å™¨ ===
    with c_editor:
        st.subheader(f"æ ‡ç­¾å€¼åˆ—è¡¨ ({selected_cat})")
        
        # è¿‡æ»¤å‡ºå½“å‰ç±»åˆ«çš„æ ‡ç­¾
        current_data = []
        for tid, tdata in all_tags.items():
            if tdata.get('category', 'General') == selected_cat:
                # åå‘æŸ¥æ‰¾é¢œè‰²åç§°
                color_hex = tdata.get('color', '#8c8c8c')
                color_name = "Neutral (ç°)"
                for k, v in COLOR_PRESETS.items():
                    if v == color_hex:
                        color_name = k
                        break
                
                current_data.append({
                    "id": tid,
                    "name": tdata.get("name", "New Tag"),
                    "style": color_name
                })
        
        # å¦‚æœå½“å‰ç±»åˆ«æ²¡æ•°æ®ï¼ŒåŠ ä¸€è¡Œç©ºçš„æ–¹ä¾¿ç¼–è¾‘
        if not current_data:
            current_data = [{"id": str(uuid.uuid4())[:8], "name": "", "style": "Neutral (ç°)"}]

        df = pd.DataFrame(current_data)

        # æ¸²æŸ“è¡¨æ ¼
        edited_df = st.data_editor(
            df,
            column_config={
                "id": None, 
                "name": st.column_config.TextColumn("æ ‡ç­¾å€¼ (Value)", required=True, width="medium"),
                "style": st.column_config.SelectboxColumn(
                    "æ ·å¼é¢„è®¾", 
                    options=list(COLOR_PRESETS.keys()),
                    required=True,
                    width="medium"
                )
            },
            num_rows="dynamic",
            use_container_width=True,
            hide_index=True,
            key=f"editor_{selected_cat}" # å…³é”®ï¼šåˆ‡æ¢ç±»åˆ«æ—¶é‡ç½®è¡¨æ ¼
        )

        # ä¿å­˜åŒº
        c_save, c_preview = st.columns([1, 3])
        if c_save.button("ğŸ’¾ ä¿å­˜å½“å‰ç»´åº¦", type="primary"):
            # 1. å…ˆæŠŠéå½“å‰ç»´åº¦çš„æ ‡ç­¾æ‹¿å‡ºæ¥ä¿ç•™
            final_tags = {k: v for k, v in all_tags.items() if v.get('category', 'General') != selected_cat}
            
            # 2. åˆå¹¶å½“å‰ç»´åº¦çš„ç¼–è¾‘ç»“æœ
            for _, row in edited_df.iterrows():
                # è·³è¿‡ç©ºè¡Œ
                if not row["name"]: continue
                
                tid = row.get("id")
                if not tid or pd.isna(tid): tid = str(uuid.uuid4())[:8]
                
                final_tags[tid] = {
                    "name": row["name"],
                    "category": selected_cat,
                    "color": COLOR_PRESETS.get(row["style"], "#8c8c8c")
                }
            
            tag_mgr.save_all_tags(final_tags)
            st.toast(f"ç»´åº¦ {selected_cat} å·²æ›´æ–°", icon="âœ…")
            st.rerun()
            
        with c_preview:
            # å®æ—¶é¢„è§ˆæ•ˆæœ
            preview_tags = []
            for _, row in edited_df.iterrows():
                if row["name"]:
                    col = COLOR_PRESETS.get(row["style"], "#888")
                    preview_tags.append(sac.Tag(label=f"{selected_cat}: {row['name']}", color=col, bordered=False))
            if preview_tags:
                sac.tags(preview_tags, align='start')

if __name__ == "__main__":
    main()

ğŸ“‚ 3. modules/cmd_generator/app.py (å¸ƒå±€ç´§å‡‘åŒ–ä¸Grafanaé£æ ¼å±•ç¤º)
è¿™é‡Œçš„é‡ç‚¹æ˜¯ï¼š
 * æ ‡ç­¾ç­›é€‰ï¼šä¹Ÿæ˜¯äºŒçº§è”åŠ¨ã€‚å…ˆé€‰ç»´åº¦ï¼Œå†é€‰å€¼ã€‚
 * å¡ç‰‡å±•ç¤ºï¼šæ ‡ç­¾æ˜¾ç¤ºä¸º ç»´åº¦: å€¼ çš„èƒ¶å›Šæ ·å¼ã€‚
 * å¸ƒå±€ï¼šé¡¶éƒ¨å·¥å…·æ å•è¡ŒåŒ–ã€‚
